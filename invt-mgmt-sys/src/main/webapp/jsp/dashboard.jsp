<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>Dashboard</title>
</head>
<body>
	<div class="row">
		<div class="col-md-3 col-sm-6 col-xs-12">
			<div class="info-box bg-aqua reportsDiv">
				<span class="info-box-icon"><i class="fa fa-users"></i></span>
				<div class="info-box-content">
					<span class="info-box-text ng-binding">4</span> <span
						class="progress-description ng-binding">Classes</span>
				</div>
				<!-- /.info-box-content -->
			</div>
			<!-- /.info-box -->
		</div>

		<div class="col-md-3 col-sm-6 col-xs-12">
			<div class="info-box bg-yellow reportsDiv">
				<span class="info-box-icon"><i class="fa fa-user"></i></span>
				<div class="info-box-content">
					<span class="info-box-text ng-binding">66</span> <span
						class="progress-description ng-binding">Students</span>
				</div>
				<!-- /.info-box-content -->
			</div>
			<!-- /.info-box -->
		</div>

		<div class="col-md-3 col-sm-6 col-xs-12">
			<div class="info-box bg-green reportsDiv">
				<span class="info-box-icon"><i class="fa fa-male"></i></span>
				<div class="info-box-content">
					<span class="info-box-text ng-binding">79</span> <span
						class="progress-description ng-binding">Teachers</span>
				</div>
				<!-- /.info-box-content -->
			</div>
			<!-- /.info-box -->
		</div>

		<div class="col-md-3 col-sm-6 col-xs-12">
			<div class="info-box bg-red reportsDiv">
				<span class="info-box-icon"><i class="fa fa-inbox"></i></span>
				<div class="info-box-content">
					<span class="info-box-text ng-binding">0</span> <span
						class="progress-description ng-binding">Messages</span>
				</div>
				<!-- /.info-box-content -->
			</div>
			<!-- /.info-box -->
		</div>
	</div>
</body>
</html>